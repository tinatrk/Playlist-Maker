<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="@dimen/create_playlist_cover_size_312"
        android:height="@dimen/create_playlist_cover_size_312"
        android:gravity="center">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/corner_radius_8" />
            <stroke
                android:width="@dimen/line_width_1"
                android:color="@color/gray_400"
                android:dashWidth="@dimen/indentation_16"
                android:dashGap="@dimen/indentation_16" />
        </shape>
    </item>

    <item
        android:width="@dimen/create_playlist_icon_size_100"
        android:height="@dimen/create_playlist_icon_size_100"
        android:drawable="@drawable/ic_add_photo_100"
        android:gravity="center" />
</layer-list>